{
  "name": "draftjs-wordpress-clone-master",
  "version": "1.0.0",
  "description": "Wordpress Clone with basic CRUD operations and Draft.js editor",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix frontend",
    "client-add": "npm install --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
