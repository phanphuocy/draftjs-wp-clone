  },
  "homepage": "https://github.com/phanphuocy/draftjs-wp-clone#readme",
  "dependencies": {
    "config": "^3.2.5",
    "express": "^4.17.3",
    "express-validator": "^6.3.1",
    "mongoose": "^5.8.9",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
